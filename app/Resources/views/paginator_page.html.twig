{% for prod in paginator %}
    <tr>
        <th>
            {{ prod.id }}
        </th>
        <td>
            <a href="javascript://" class="product-item" data-id="{{ prod.id }}">{{ prod.name }}</a>
        </td>
        <td>
            {{ prod.description }}
        </td>
        <td>
            {{ prod.price }}
        </td>
        <td>
            {#<a href="products/edit/{{ prod.id }}">Edit</a>
            <a href="products/remove/{{ prod.id }}"
               onclick="if(!confirm('Are you shure?')) return false;">Remove</a>#}
        </td>
    </tr>
{% endfor %}